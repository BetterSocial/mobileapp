{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "@typescript-eslint/object-curly-spacing": {
      "severity": "warning"
    },
    "no-var-requires": true,
    "max-classes-per-file": false,
    "no-console": {
      "severity": "warning"
    },
    "no-trailing-whitespace": false,
    "new-parens": false
  },
  "rulesDirectory": []
}
