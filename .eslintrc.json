{
    "env": {
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "parserOptions": {
            "ecmaFeatures": {
                "jsx": true,
                "modules": true
            }
        },
        "plugins": ["import", "@typescript-eslint","prettier", "react", "react-hooks", "react-native"]
    },
    "rules": {
        "linebreak-style": "off",
        "no-console": "off",
        "import/prefer-default-export": "off",
        "no-use-before-define": "warn"
    }
}
