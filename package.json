  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "<rootDir>/jestSetup.js"
    ],
    "moduleDirectories": [
      "<rootDir>/node_modules",
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "\\.svg": "<rootDir>/__mocks__/svgMock.js"
    },
    "collectCoverageFrom": [
      "__tests__",
      "src/**/*.{js,jsx}"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx}": [
      "yarn eslint --cache --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
