{
  "name": "bettersocial",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:dev-debug": "cd android && ./gradlew clean && cd .. && ENVFILE=.env.development npx react-native run-android --variant=devdebug",
    "android:prod-debug": "cd android && ./gradlew clean && cd .. && ENVFILE=.env.production npx react-native run-android --variant=proddebug",
    "android:dev": "react-native run-android --variant=devDebug --appIdSuffix=dev",
    "android:prod": "react-native run-android --variant=prodDebug --appIdSuffix=devprod",
    "build:apk-android-dev": "cd android && NODENV=development ENVFILE=.env.development ./gradlew assembledevrelease",
    "build:apk-android-dev-update-stream": "yarn update-stream && cd android && ./gradlew clean && ENVFILE=.env.development ./gradlew assembledevrelease",
    "build:apk-android-prod": "cd android && ENVFILE=.env.production ./gradlew assembleprodrelease",
    "build:apk-android-prod-update-stream": "yarn update-stream && cd android && ./gradlew clean && ENVFILE=.env.production ./gradlew assembleprodrelease",
    "build:apk-android-profileable": "cd android && ENVFILE=.env.production ./gradlew assembleprodprofileable",
    "build:apk-android-profileable-update-stream": "yarn update-stream && cd android && ./gradlew clean && ENVFILE=.env.production ./gradlew assembleprodprofileable",
    "build:bundle-android-dev": "npm run update-env-dev && npm run update-stream && cd android && ./gradlew clean && ENVFILE=.env.development ./gradlew bundledevrelease",
    "build:bundle-android-prod": "cd android && ENVFILE=.env.production ./gradlew bundleprodrelease",
    "build:bundle-android-prod-update-stream": "npm run update-env-prod && npm run update-stream && cd android && ./gradlew clean && ENVFILE=.env.production ./gradlew bundleprodrelease",
    "ios:dev-debug": "npm run update-env-dev && react-native run-ios --scheme 'BetterSocialDev' --simulator='iPhone 13'",
    "start": "react-native start",
    "test": "jest --verbose --collectCoverage",
    "testCoverage": "jest --verbose --coverage --collectCoverageFrom=\"./src/**\" __tests__ src",
    "test:update-snapshot": "jest __tests__ src -u",
    "test:content": "jest __tests__/screens/FeedScreen/ContentPoll.test.js -u && jest __tests__/screens/FeedScreen/ContentLink.test.js -u",
    "lint": "eslint .",
    "format": "npx prettier --check ./src",
    "android:clean": "cd android && ./gradlew clean && ./gradlew cleanBuildCache",
    "reinstall": "rm -rf node_modules && yarn install",
    "reset": "react-native start --reset-cache",
    "prepare": "husky install",
    "reset-all": "rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf node_modules && npm cache clean --force && npm install && npm start -- --reset-cache",
    "update-stream": "rm -rf yarn.lock && yarn cache clean && rm -rf node_modules/stream-chat && rm -rf node_modules/stream-chat-react-native-core && yarn",
    "update-env-dev": "doppler secrets download -p bettersocial --no-file --format env-no-quotes --config dev > .env.development",
    "update-env-prod": "doppler secrets download -p bettersocial --no-file --format env-no-quotes --config prd > .env.production",
    "postinstall": "patch-package"
  },
