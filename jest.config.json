{
  "preset": "react-native",
  "transformIgnorePatterns": [
    "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|rollbar-react-native|@fortawesome|@react-native|@react-navigation|@react-native-firebase|stream-chat-react-native)"

  ],
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "verbose": true,
  "setupFilesAfterEnv": [
    "<rootDir>/__mocks__/jest-mock.ts"
    ]
}